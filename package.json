  "bugs": {
    "url": "https://github.com/niwamaxou/ninibot/issues"
  },
  "homepage": "https://github.com/niwamaxou/ninibot",
  "dependencies": {
    "discord.js": "^7.0.1",
    "random-js": "^1.0.8",
    "request": "2.72.0",
    "xml2json": "0.9.1",
    "ytdl-core": "^0.7.16"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.7.4",
    "babel-loader": "6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "6.6.0",
    "jshint": "^2.9.2",
